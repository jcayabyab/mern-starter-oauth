{
  "name": "mern-starter",
  "version": "1.0.0",
  "description": "A personalized boilerplate to use with MERN applications. Includes dependencies for multiple features, such as Mongoose, Passport.js, axios, etc.",
  "main": "index.js",
  "engines": {
    "node": "8.11.3",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCayabyab/mern-starter-oauth.git"
  },
  "author": "Jofred Cayabyab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JCayabyab/mern-starter-oauth/issues"
  },
  "homepage": "https://github.com/JCayabyab/mern-starter-oauth#readme",
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.17.3",
    "mongoose": "^5.4.5",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0"
  }
}
